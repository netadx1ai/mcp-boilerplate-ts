{
  "name": "mcp-client-example",
  "version": "1.0.0",
  "description": "Example MCP client demonstrating integration with TypeScript boilerplate servers",
  "type": "module",
  "main": "dist/mcp-client-example.js",
  "scripts": {
    "dev": "ts-node mcp-client-example.ts",
    "build": "tsc",
